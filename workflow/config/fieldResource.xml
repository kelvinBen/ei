<?xml version="1.0" encoding="UTF-8"?>
<fields selectSplit="、">
	<!--域的标准参数有：${name},${formTitle},${title},${notnull},${fieldValue},${restriction},${append}-->
	<field name="calculate" text="计算域" type="字段" conType="Map">
		<common text="普通域代码" name="default">
			<![CDATA[<input class="" type="text" name="${name}" formTitle="${formTitle}" title="${title}" notnull="${notnull}" value="${fieldValue}" sumField="${sumField?if_exists}"  FUNC="${FUNC?if_exists}"  FUNARG="${FUNARG?if_exists}"  restriction="${restriction}" >${append}]]>
		</common>
		<readonly text="只读域代码" name="default">
			<![CDATA[<span id="${name}" class="" title="${title}">${fieldValue}</span>]]>
		</readonly>
		<search text="搜索域代码">
			<![CDATA[<input class="" type="text" name="${name}" formTitle="${formTitle}" title="${title}" restriction="${restriction}" >]]>
		</search>
		<append prefix="false" text="必填字段的附加显示,prefix为true表示在标题以前" name="default">
			<![CDATA[<b><font size="2" color="#FF0000">*</font></b>]]>
		</append>
	</field>
	<!--扩展参数：${key},${description},list的名字:listValue ${fieldValue}为list-->
	<field name="check" text="复选框" type="字段" conType="List-Map">
		<common text="普通域代码" name="default">
			<![CDATA[<table border="0" width="100%" cellspacing="0" cellpadding="0">
			<#list listValue as lv>
			<tr>
			<td width="1" style="border:0 solid;"><input type="checkbox" description="${lv.description}" class="" name="${name}" notnull="${notnull}" formTitle="${formTitle}" sumField="${sumField?if_exists}"  <#if style?if_exists!="">style="${style?if_exists}"</#if> FUNC="${FUNC?if_exists}"  FUNARG="${FUNARG?if_exists}"  title="${title}"<#list fieldValue as v><#if lv.key = v> checked</#if></#list> value="${lv.key}"></td>
			<td nowrap style="border:0 solid;">${lv.description}${append}</td></tr>
			</#list>
			</table>]]>
		</common>
		<readonly text="只读域代码" name="default">
			<![CDATA[<span id="${name}" class="" title="${title}"><#list listValue as lv><#list fieldValue as v><#if lv.key = v>${lv.description}&nbsp;&nbsp;&nbsp;</#if></#list></#list></span>]]>
		</readonly>
		<search text="搜索域代码">
			<![CDATA[<table border="0" width="100%" cellspacing="0" cellpadding="0">
			<#list listValue as lv>
			<tr>
			<td width="1" style="border:0"><input type="checkbox" class="" name="${name}" <#if style?if_exists!="">style="${style?if_exists}"</#if> formTitle="${formTitle}" title="${title}" value="${lv.key}"></td>
			<td nowrap style="border:0">${lv.description}</td></tr>
			</#list>
			</table>]]>
		</search>
		<append prefix="false" text="必填字段的附加显示" name="default">
			<![CDATA[<b><font size="2" color="#FF0000">*</font></b>]]>
		</append>
	</field>
	<!--扩展参数：${dateType}的值为枚举：timestamp,date,time-->
	<field name="dataTime" text="日期时间选择" type="字段" conType="Map">
		<common text="普通域代码" name="default">
			<![CDATA[<table border="0"  cellspacing="0" cellpadding="0"><tr>
		<td  nowrap style="border:0 solid;"><input class="" onDblClick="clearDateTime(document.all('${name}'));" type="text" name="${name}" formTitle="${formTitle}" title="${title}"  sumField="${sumField?if_exists}"  FUNC="${FUNC?if_exists}"  FUNARG="${FUNARG?if_exists}" notnull="${notnull}" value="${fieldValue}" restriction="${restriction}" readOnly="true"><#if dateType = "time">${append}</#if><#if dateType != "time"><button class="date_button" onclick="<#if dateType = "timestamp">getDateTime('${name}');</#if><#if dateType = "date">getDate('${name}');</#if>"><img class="" title="${title}" src="skins/default/images/date/time.gif" style="cursor:pointer;"></button>${append}
		</#if>
		</td>
		</tr></table>]]>
		</common>
		<readonly text="只读域代码" name="default">
			<![CDATA[<span id="${name}" class="" title="${title}">${fieldValue}</span><input type="hidden" name="${name}" value="${fieldValue}">]]>
		</readonly>
		<search text="搜索域代码">
			<![CDATA[<table border="0" cellspacing="0" cellpadding="0"><tr>
		<td  nowrap style="border:0 solid;"><input class="" type="text" name="${name}_start" formTitle="${formTitle}" title="${title}[开始]" restriction="${restriction}" readOnly="true"></td>
		<#if dateType != "time">
		<td  style="border:0 solid;"><button class="date_button" onclick="<#if dateType = "timestamp">getDateTime('${name}_start');</#if><#if dateType = "date">getDate('${name}_start');</#if>" ><img class="" title="${title}" src="skins/default/images/date/time.gif" style="cursor:pointer;"></button></td>
		</#if>
		<td  nowrap style="border:0 solid;"><input class="" type="text" name="${name}_end" formTitle="${formTitle}" title="${title}[结束]" restriction="${restriction}" readOnly="true"></td>
		<#if dateType != "time">
		<td  style="border:0 solid;"><button class="date_button" onclick="<#if dateType = "timestamp">getDateTime('${name}_end');</#if><#if dateType = "date">getDate('${name}_end');</#if>"><img class="" title="${title}" src="skins/default/images/date/time.gif" style="cursor:pointer;"></button></td>
		</#if>
		</tr></table>]]>
		</search>
		<append prefix="false" text="必填字段的附加显示" name="default">
			<![CDATA[<b><font size="2" color="#FF0000">*</font></b>]]>
		</append>
	</field>
	<field name="display" text="显示域" type="字段" conType="Map">
		<common text="普通域代码" name="default">
			<![CDATA[<span id="${name}" class="" title="${title}" sumField="${sumField?if_exists}"  FUNC="${FUNC?if_exists}"  FUNARG="${FUNARG?if_exists}" >${fieldValue}</span>]]>
		</common>
		<readonly text="只读域代码" name="default">
			<![CDATA[<span id="${name}" class="" title="${title}">${fieldValue}</span>]]>
		</readonly>
	</field>
	<!--扩展参数：${key},${description},list的名字:listValue-->
	<field name="dropDown" text="下拉列表" type="字段" conType="List-Map">
		<common text="普通域代码" name="default">
			<![CDATA[<select class="" name="${name}" formTitle="${formTitle}" title="${title}" notnull="${notnull}" <#if style?if_exists!="">style="${style?if_exists}"</#if> sumField="${sumField?if_exists}"  FUNC="${FUNC?if_exists}"  FUNARG="${FUNARG?if_exists}" >
			<#list listValue as lv>
			<option value="${lv.key}" <#if lv.key = fieldValue >selected</#if>>${lv.description}</option>
			</#list>
			</select>${append}]]>
		</common>
		<readonly text="只读域代码" name="default">
			<![CDATA[<span id="${name}" class="" title="${title}">
			<#list listValue as lv>
			<#if lv.key = fieldValue >
			<input type="hidden" name="${name}" value="${lv.key}">
			${lv.description}
			</#if>
			</#list>
			</span>
			]]>
		</readonly>
		<search text="搜索域代码">
			<![CDATA[<select class="" name="${name}" formTitle="${formTitle}" title="${title}" <#if style?if_exists!="">style="${style?if_exists}"</#if>>
			<option value="" selected></option>
			<#list listValue as lv>
			<option value="${lv.key}">${lv.description}</option>
			</#list>
			</select>]]>
		</search>
		<append prefix="false" text="必填字段的附加显示" name="default">
			<![CDATA[<b><font size="2" color="#FF0000">*</font></b>]]>
		</append>
	</field>
	<!--扩展参数：${editUrl},${hiddenField}这是隐藏域列表，样式：,'aa','bb'-->
	<field name="edit" text="编辑域" type="字段" conType="Map">
		<common text="普通域代码" name="default">
			<![CDATA[<table border="0" cellspacing="0" cellpadding="0"><tr>
						<td nowrap style="border:0 solid;"><img border="0" src="skins/default/images/liulan.gif" title="${title}" onclick="editField('${editUrl}','display_${name}','${name}','${hiddenField}');" style="cursor: pointer;">${append}<textarea class="" name="${name}" formTitle="${formTitle}" style="display:none" notnull="${notnull}">${fieldValue}</textarea></td>
					</tr><tr><td style="border:0 solid;"><span id="display_${name}" title="${title}">${fieldDecodeValue}</span></td>
					</tr>
				</table>
		]]>
		</common>
		<common text="公文字号" name="edocWord">
			<![CDATA[<table border="0" cellspacing="0" cellpadding="0"><tr>
						<td style="border:0 solid;"><input class="" name="${name}" formTitle="${formTitle}"  notnull="${notnull}" title="${title}" value="${fieldValue}" <#if style?if_exists!="">style="${style?if_exists}"</#if>"></td>
						<td nowrap width="100%" style="border:0 solid;"><img border="0" src="skins/default/images/liulan.gif" title="${title}" onclick="editField('${editUrl}',null,'${name}','${hiddenField}');" style="cursor: pointer;">${append}</td>
					</tr>
				</table>
		]]>
		</common>
		<common text="普通字号" name="commonWord">
			<![CDATA[<table border="0" cellspacing="0" cellpadding="0"><tr>
						<td style="border:0 solid;"><input class="" name="${name}" formTitle="${formTitle}"  notnull="${notnull}" title="${title}" value="${fieldValue}" <#if style?if_exists!="">style="${style?if_exists}"</#if>></td>
						<td nowrap width="100%" style="border:0 solid;"><img border="0" src="skins/default/images/liulan.gif" title="${title}" onclick="editField('${editUrl}',null,'${name}','${hiddenField}');" style="cursor: pointer;">${append}</td>
					</tr>
				</table>
		]]>
		</common>
		<readonly text="只读域代码" name="default">
			<![CDATA[<span id="display_${name}" title="${title}">${fieldDecodeValue}</span>
			<textarea name="${name}" formTitle="${formTitle}" style="display:none">${fieldValue}</textarea>]]>
		</readonly>
		<readonly text="特定类型的只读域代码" name="name1">
			<![CDATA[<span id="display_${name}" title="${title}">${fieldDecodeValue}</span>
			<textarea name="${name}" formTitle="${formTitle}" style="display:none">${fieldValue}</textarea>]]>
		</readonly>
		<hidden name="default" text="隐藏域代码">
			<![CDATA[<textarea name="${name}" formTitle="${formTitle}" style="display:none">${fieldValue}</textarea>]]>
		</hidden>
		<append prefix="false" text="必填字段的附加显示" name="default">
			<![CDATA[<b><font size="2" color="#FF0000">*</font></b>]]>
		</append>
	</field>
	<field name="file" text="文件域" type="字段" conType="Map">
		<common text="普通域代码" name="default">
			<![CDATA[<input type="file" class="" name="${name}" formTitle="${formTitle}" title="${title}" value="${fieldValue}" notnull="${notnull}">${append}]]>
		</common>
		<readonly text="只读域代码" name="default">
			<![CDATA[<span id="${name}" title="${title}">${fieldValue}</span>]]>
		</readonly>
		<append prefix="false" text="必填字段的附加显示" name="default">
			<![CDATA[<b><font size="2" color="#FF0000">*</font></b>]]>
		</append>
	</field>
	<field name="hidden" text="隐藏域" type="字段" conType="Map">
		<common text="普通域代码" name="default">
			<![CDATA[<input type="hidden" name="${name}" formTitle="${formTitle}" title="${title}" value="${fieldValue}">]]>
		</common>
	</field>
	<!--扩展参数：${description},${target} ${targetWindow}-->
	<field name="hyperLink" text="超链接" type="字段" conType="Map">
		<common text="普通域代码" name="default">
			<![CDATA[<a id="${name}" href="${target}" class="" title="${title}" target="${targetWindow}">${description}</a>]]>
		</common>
		<readonly text="只读域代码" name="default">
			<![CDATA[<span id="${name}" title="${title}">${description}</span>]]>
		</readonly>
	</field>
	<!--扩展参数：${border},${height} ${WIDTH?if_exists}${src}-->
	<field name="image" text="图像" type="字段" conType="Map">
		<common text="普通域代码" name="default">
			<![CDATA[<img id="${name}" class="" title="${title}" src="${src}" border="${border}" <#if height != "0">height="${height}"</#if> <#if width != "0">width="${width?if_exists}"</#if>>]]>
		</common>
		<readonly text="只读域代码" name="default">
			<![CDATA[<img id="${name}" class="" title="${title}" src="${src}" border="${border}" <#if height != "0">height="${height}"</#if> <#if width != "0">width="${width?if_exists}"</#if>>]]>
		</readonly>
	</field>
	<!--扩展参数：${key},${description},list的名字:listValue-->
	<field name="radio" text="单选域" type="字段" conType="List-Map">
		<common text="普通域代码" name="default">
			<![CDATA[<table border="0" width="100%" cellspacing="0" cellpadding="0">
			<#list listValue as lv>
			<tr>
			<td width="1" style="border:0 solid;"><input type="radio" description="${lv.description}" notnull="${notnull}" class="" name="${name}" formTitle="${formTitle}"  sumField="${sumField?if_exists}" <#if style?if_exists!="">style="${style?if_exists}"</#if>  FUNC="${FUNC?if_exists}"  FUNARG="${FUNARG?if_exists}" title="${title}"<#if lv.key = fieldValue> checked</#if> value="${lv.key}"></td>
			<td nowrap style="border:0 solid;">${lv.description}${append}</td></tr>
			</#list></table>]]>
		</common>
		<readonly text="只读域代码" name="default">
			<![CDATA[<span id="${name}" class="" title="${title}"><#list listValue as lv><#if lv.key = fieldValue>${lv.description}&nbsp;&nbsp;&nbsp;</#if></#list></span>]]>
		</readonly>
		<search text="搜索域代码">
			<![CDATA[<table border="0" width="100%" cellspacing="0" cellpadding="0">
			<#list listValue as lv>
			<tr>
			<td width="1" style="border:0 solid;"><input type="radio" class="" name="${name}" <#if style?if_exists!="">style="${style?if_exists}"</#if> formTitle="${formTitle}" title="${title}" value="${lv.key}"></td>
			<td nowrap style="border:0 solid;">${lv.description}</td></tr>
			</#list></table>]]>
		</search>
		<append prefix="false" text="必填字段的附加显示" name="default">
			<![CDATA[<b><font size="2" color="#FF0000">*</font></b>]]>
		</append>
	</field>
	<!--扩展参数：${src} ${editUrl},list的名字:listValue = ${displayText},${displayUrl}-->
	<field name="select" text="选择域" type="字段">
		<common text="普通域代码" name="default">
			<![CDATA[<table border="0" cellspacing="0" cellpadding="0" width="100%">
			<tr>
			<td nowrap style="border:0 solid;"><textarea name="${name}" formTitle="${formTitle}" style="display:none" notnull="${notnull}">${fieldValue}</textarea>${append}</td>
		  <td style="border:0 solid;"><img title="${title}" src="<#if src = "">skins/default/images/liulan.gif<#else>${src}</#if>" border="0" onclick="selectField('${editUrl}',document.all('${name}'),window.display_${name});" style="cursor: pointer;"><span id="display_${name}" title="${title}"><#list listValue as lv><a href="${lv.displayUrl}" title="查看详细信息" target="main">${lv.displayText}</a></#list></span></td>
			
			</tr>
		</table>
		]]>
		</common>
		<common text="特定类型的普通域代码" name="name1">
			<![CDATA[<table border="0" cellspacing="0" cellpadding="0" width="100%">
			<tr>
			<td nowrap style="border:0 solid;"><textarea name="${name}" formTitle="${formTitle}" style="display:none" notnull="${notnull}">${fieldValue}</textarea><img title="${title}" src="<#if src = "">skins/default/images/liulan.gif<#else>${src}</#if>" border="0" onclick="selectField('${editUrl}',document.all('${name}'),window.display_${name});" style="cursor: pointer;">${append}</td>
		  <td style="border:0 solid;"><span id="display_${name}" title="${title}"><#list listValue as lv><a href="${lv.displayUrl}" title="查看详细信息" target="main">${lv.displayText}</a></#list></span></td>
			
			</tr>
		</table>
		]]>
		</common>
		<readonly text="只读域代码" name="default">
			<![CDATA[<span id="display_${name}" title="${title}"><#list listValue as lv><a href="${lv.displayUrl}" title="查看详细信息" target="main">${lv.displayText}</a></#list></span><textarea name="${name}" formTitle="${formTitle}" style="display:none">${fieldValue}</textarea>]]>
		</readonly>
		<readonly text="特定类型的只读域代码" name="name1">
			<![CDATA[<span id="display_${name}" title="${title}"><#list listValue as lv><a href="${lv.displayUrl}" title="查看详细信息" target="main">${lv.displayText}</a></#list></span><textarea name="${name}" formTitle="${formTitle}" style="display:none">${fieldValue}</textarea>]]>
		</readonly>
		<search text="搜索域代码">
			<![CDATA[<table border="0" cellspacing="0" cellpadding="0">
				<tr>
			    
				<td nowrap style="border:0 solid;"><textarea name="${name}" formTitle="${formTitle}" style="display:none" ></textarea><img title="${title}" src="<#if src = "">skins/default/images/liulan.gif<#else>${src}</#if>" border="0" onclick="selectField('${editUrl}',document.all('${name}'),window.display_${name});" style="cursor: pointer;"></td>
				<td nowrap style="border:0 solid;"><span id="display_${name}" title="${title}"></span></td>
				</tr>
			</table>
			]]>
		</search>
		<append prefix="false" text="必填字段的附加显示" name="default">
			<![CDATA[<b><font size="2" color="#FF0000">*</font></b>]]>
		</append>
	</field>
			<!--扩展参数：${displayValue},${editUrl},${signType}-->
	<field name="special" text="特殊编辑域" type="字段">
	   <common text="图片签名" name="signpic">
			      <![CDATA[<table border="0" width="100%" cellspacing="0" cellpadding="0">
			     <tr>
			      <td width="300" style="border:0">
			       <span id = "display_${name}" title="${title}">${displayValue}</span>
			      </td>
			     </tr>
			     <tr>
			      <td style="border:0"><img border="0" src="skins/default/images/liulan.gif" title="${title}" onclick="signField('${editUrl}',document.all('${name}'),document.all('last_${name}'),'${name}','650px','400px','${signType}');" style="cursor: pointer;">${append}</td>
			     </tr>
			     <tr>
			      <td style="border:0"><textarea class="" name="last_${name}" rows="6" cols="28" readonly style="display:none;" onclick="signField('${editUrl}',document.all('${name}'),document.all('last_${name}'),'${name}','650px','400px','${signType}');" ></textarea><textarea style="display:none" name="${name}" title="${title}" formTitle="${formTitle}" notnull="${notnull}" >${fieldValue}</textarea></td>
			     </tr>
			    </table>
			    <script language="javascript">
			      displaySign(window.display_${name},document.all("${name}").value,document.all("last_${name}"),"${name}",'${signType}');
			    </script>]]>
		</common>
		<common text="普通域代码" name="default">
			<![CDATA[<table border="0" width="100%" cellspacing="0" cellpadding="0">
					<tr>
						<td width="100%" style="border:0;">
							<input type="hidden" name="dealOpinion" valueCtrl="${name}"  signType="0" />
							<span id = "display_${name}" title="${title}">${displayValue}</span>
						</td>
					</tr>
					<tr>
						<td style="border:0"><img border="0" src="skins/default/images/liulan.gif" title="${title}" onclick="specialEditField('${editUrl}',document.all('${name}'));" style="cursor: pointer;">${append}</td>
					</tr>
					<tr>
						<td style="border:0"><textarea class="" name="${name}" title="${title}" formTitle="${formTitle}" rows="5" cols="35" readonly onclick="specialEditField('${editUrl}',document.all('${name}'));" notnull="${notnull}" <#if style?if_exists!="">style="${style?if_exists}"</#if>>${fieldValue}</textarea></td>
					</tr>
				</table>]]>
		</common>
		<common text="带有弹出框,信息按照一定格式组织" name="format_option">
			<![CDATA[<table border="0" width="100%" cellspacing="0" cellpadding="0">
					<tr>
						<td width="100%" style="border:0">
							<input type="hidden" name="dealOpinion" valueCtrl="${name}"  signType="0" />
							<span id = "display_${name}" title="${title}">${displayValue}</span>
						</td>
					</tr>
					<tr>
						<td style="border:0"><input name="make_out_${name}" value="填写" type="button" onclick="putOption( '${editUrl?if_exists}', document.all('${name}') , '${name}' , 'option_${name}');"></td>
					</tr>
					<tr>
						<td style="height:22;width:100;word-wrap:break-word;border:0"><textarea name="${name}" title="${title}" formTitle="${formTitle}" rows="5" cols="35" readonly notnull="${notnull}" <#if style?if_exists!="">style="${style?if_exists}"</#if>>${fieldValue?if_exists}</textarea></td>
						<td style="border:0"><textarea style="display:none;" name="option_${name}"></textarea></td>
					</tr>
					<script language="javascript">
						initFormatOption(document.all('${name}') , document.all('option_${name}'));
					</script>
				</table>]]>
		</common>
		<common text="填充默认值" name="default_button">
			<![CDATA[<table border="0" width="100%" cellspacing="0" cellpadding="0">
					<tr>
						<td width="100%" style="border:0">
							<input type="hidden" name="dealOpinion" valueCtrl="${name}"  signType="0" />
							<span id = "display_${name}" title="${title}">${displayValue}</span>
						</td>
					</tr>
					<tr>
						<td style="border:0"><img border="0" src="skins/default/images/liulan.gif" title="${title}" onclick="specialEditField('${editUrl}',document.all('${name}'));" style="cursor: pointer;">
						<input name="setDefault_${name}" value="签名" type="button" onclick="defaultSign('${name}');">${append}</td>
					</tr>
					<tr>
						<td style="border:0"><textarea class="" name="${name}" title="${title}" formTitle="${formTitle}" rows="4" cols="35" readonly onclick="specialEditField('${editUrl}',document.all('${name}'));" notnull="${notnull}" <#if style?if_exists!="">style="${style?if_exists}"</#if>>${fieldValue?if_exists}</textarea></td>
					</tr>
				</table>]]>
		</common>
		<common text="手签/签名域" name="sign">
			<![CDATA[<table border="0" width="100%" cellspacing="0" cellpadding="0">
					<tr>
						<td width="200" style="border:0">
							<span id = "display_${name}" title="${title}">${displayValue}</span>
						</td>
					</tr>
					<tr>
						<td style="border:0"><img border="0" src="skins/default/images/liulan.gif" title="${title}" onclick="signField('${editUrl}',document.all('${name}'),document.all('last_${name}'),'${name}','650px','400px','${signType}');" style="cursor: pointer;">${append}</td>
					</tr>
					<tr>
